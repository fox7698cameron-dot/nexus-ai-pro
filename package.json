{
  "name": "nexus-ai-pro",
  "version": "2.0.0",
  "description": "Military-grade encrypted AI platform with multi-model support, game/app development, n8n-style automation, and customizable avatars",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "nodemon server.js",
    "build": "vite build",
    "start": "node server.js",
    "preview": "vite preview",
    "security:scan": "node -e \"import('./server.js').then(m => m.security?.scanVulnerabilities())\"",
    "security:patch": "node -e \"import('./server.js').then(m => m.security?.autoPatch())\"",
    "lint": "eslint . --ext .js,.jsx",
    "test": "vitest",
    "test:security": "vitest run --grep security",
    "docker:build": "docker build -t nexus-ai-pro .",
    "docker:run": "docker run -p 3001:3001 --env-file .env nexus-ai-pro",
    "docker:compose": "docker-compose up -d"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "openai": "^4.56.0",
    "socket.io": "^4.7.5",
    "uuid": "^10.0.0",
    "winston": "^3.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.5.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "vite": "^5.4.2",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ai",
    "chatbot",
    "claude",
    "gpt-4",
    "gemini",
    "multi-model",
    "encryption",
    "security",
    "automation",
    "workflow",
    "game-development",
    "app-development"
  ],
  "author": "Nexus AI Team",
  "license": "MIT"
}
